          </text>
          <view class="_view data-v-d8d177a2 mx-modal-header-colse" bindtap="handleProxy" data-eventid="{{'TGk-2'}}" data-comkey="{{$k}}">
            <text class="_text data-v-d8d177a2 icon icon-close"></text>
          </view>
        </view>
        <view wx:else class="_view data-v-d8d177a2 mx-modal-header">
