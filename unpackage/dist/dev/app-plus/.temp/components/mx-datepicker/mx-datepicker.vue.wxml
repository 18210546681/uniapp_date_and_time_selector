            <picker-view class="_picker-view data-v-d8d177a2 mx-time" indicator-style="height: 22px;" value="{{time}}" bindchange="handleProxy" data-eventid="{{'GTY-5'}}" data-comkey="{{$k}}">
              <picker-view-column class="_picker-view-column data-v-d8d177a2">
                <view wx:key="value" key="{{value}}" wx:for="{{24}}" wx:for-index="index" wx:for-item="value" class="_view data-v-d8d177a2">{{value
                  <10? '0'+value:value}}时</view>
              </picker-view-column>
              <picker-view-column class="_picker-view-column data-v-d8d177a2">
                <view wx:key="value" key="{{value}}" wx:for="{{60}}" wx:for-index="index" wx:for-item="value" class="_view data-v-d8d177a2">{{value
                  <10? '0'+value:value}}分</view>
              </picker-view-column>
              <picker-view-column class="_picker-view-column data-v-d8d177a2">
                <view wx:key="value" key="{{value}}" wx:for="{{60}}" wx:for-index="index" wx:for-item="value" class="_view data-v-d8d177a2">{{value
                  <10? '0'+value:value}}秒</view>
              </picker-view-column>
            </picker-view>
            </view>
            </view>
            <view style="color: #666;font-size: 14px;padding: 10px 0;" class="_view data-v-d8d177a2"> 已选择:
              <text wx:if="{{type!='time'}}" class="_text data-v-d8d177a2"> {{date.year
                <10?( '0'+date.year):date.year}}年 {{date.month<10?( '0'+date.month):date.month}}月 {{date.date<10?( '0'+date.date):date.date}}日 </text>
                  <text wx:if="{{type=='datetime' || type=='time'}}" class="_text data-v-d8d177a2"> {{time[0]
                    <10?( '0'+time[0]):time[0]}}: {{time[1]<10?( '0'+time[1]):time[1]}}: {{time[2]<10?( '0'+time[2]):time[2]}} </text>
            </view>
            <view class="_view data-v-d8d177a2">
              <view class="_view data-v-d8d177a2 mx-btn" bindtap="handleProxy" data-eventid="{{'vOo-6'}}" data-comkey="{{$k}}">确定</view>
            </view>
            </view>
          </view>
        </view>
</template>