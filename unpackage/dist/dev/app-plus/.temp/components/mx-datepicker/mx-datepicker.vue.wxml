              <view wx:key="index" key="{{index}}" wx:for="{{['一','二','三','四','五','六','日']}}" wx:for-index="index" wx:for-item="header" class="_view data-v-d8d177a2">{{header}}</view>
            </view>
            <view class="_view data-v-d8d177a2 mx-date-body">
              <view wx:key="index" key="{{index}}" id="{{index}}" bindtap="handleProxy" data-eventid="{{'Ekm-4-'+index}}" data-comkey="{{$k}}" wx:for="{{dateList}}" wx:for-index="index" wx:for-item="item" class="_view data-v-d8d177a2 {{[item.selected ? 'active' : '', item.is_gray ? 'gray' : '']}}">{{item.date}}</view>
            </view>
          </view>
          <view wx:if="{{type=='datetime' || type=='time'}}" class="_view data-v-d8d177a2">
